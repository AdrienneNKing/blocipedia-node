<!doctype html>
<html>
  <head>
    <% include ../static/partials/head.ejs %>
  </head>
  <body>
    <% include ../static/partials/navbar.ejs %>
      <h1>Welcome, <%= currentUser.email %>. You are a Collaborator in the following Wikis:.</h1>
      <ul class="list-group">
        <% wikis.forEach((wiki) => { %>
          <li class="list-group-item">
            <a href="/publicWikis/<%= wiki.id %>">
              <%= wiki.title %>
            </a>
          </li>
        <% }) %>
      </ul>
    <% include ../static/partials/baseScripts.ejs %>
  </body>
</html>
